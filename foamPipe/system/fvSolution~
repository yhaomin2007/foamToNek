    kappaMin          kappaMin [1 1 -3 -1 0 0 0] 0.01;
    kappaMax          kappaMax [1 1 -3 -1 0 0 0] 10.0;
    mu_user	    mu_user [1 -1 -1 0 0] 0;
    transonic       no;
